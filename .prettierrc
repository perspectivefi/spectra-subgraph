{
    "tabWidth": 4,
    "semi": false,
    "singleQuote": false,
    "bracketSpacing": true,
    "useTabs": false,
    "overrides": [
        {
            "files": "*.graphql",
            "options": {
                "printWidth": 80
            }
        },
        {
            "files": "*.json",
            "options": {
                "printWidth": 80
            }
        }
    ],
    "importOrder": [
        "^@*/(.*)$",
        "^[./]"
    ],
    "importOrderSeparation": true
}
